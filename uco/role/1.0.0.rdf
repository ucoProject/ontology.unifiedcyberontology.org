<?xml version="1.0" encoding="UTF-8"?>

<rdf:RDF
	xmlns:core="https://ontology.unifiedcyberontology.org/uco/core/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:role="https://ontology.unifiedcyberontology.org/uco/role/"
	xmlns:sh="http://www.w3.org/ns/shacl#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://ontology.unifiedcyberontology.org/uco/role">
		<rdfs:label xml:lang="en">uco-role</rdfs:label>
		<owl:imports rdf:resource="https://ontology.unifiedcyberontology.org/uco/core/1.0.0"/>
		<owl:incompatibleWith rdf:resource="https://ontology.unifiedcyberontology.org/uco/role/0.9.1"/>
		<owl:ontologyIRI rdf:resource="https://ontology.unifiedcyberontology.org/uco/role"/>
		<owl:priorVersion rdf:resource="https://ontology.unifiedcyberontology.org/uco/role/0.9.1"/>
		<owl:versionIRI rdf:resource="https://ontology.unifiedcyberontology.org/uco/role/1.0.0"/>
	</owl:Ontology>
	
	<rdf:Description rdf:about="https://ontology.unifiedcyberontology.org/uco/role/BenevolentRole">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
		<rdf:type rdf:resource="http://www.w3.org/ns/shacl#NodeShape"/>
		<rdfs:subClassOf rdf:resource="https://ontology.unifiedcyberontology.org/uco/role/Role"/>
		<rdfs:label xml:lang="en">BenevolentRole</rdfs:label>
		<rdfs:comment xml:lang="en">A benevolent role is a role with positive and/or beneficial intent.</rdfs:comment>
		<sh:targetClass rdf:resource="https://ontology.unifiedcyberontology.org/uco/role/BenevolentRole"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="https://ontology.unifiedcyberontology.org/uco/role/MaliciousRole">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
		<rdf:type rdf:resource="http://www.w3.org/ns/shacl#NodeShape"/>
		<rdfs:subClassOf rdf:resource="https://ontology.unifiedcyberontology.org/uco/role/Role"/>
		<rdfs:label xml:lang="en">MaliciousRole</rdfs:label>
		<rdfs:comment xml:lang="en">A malicious role is a role with malevolent intent.</rdfs:comment>
		<sh:targetClass rdf:resource="https://ontology.unifiedcyberontology.org/uco/role/MaliciousRole"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="https://ontology.unifiedcyberontology.org/uco/role/NeutralRole">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
		<rdf:type rdf:resource="http://www.w3.org/ns/shacl#NodeShape"/>
		<rdfs:subClassOf rdf:resource="https://ontology.unifiedcyberontology.org/uco/role/Role"/>
		<rdfs:label xml:lang="en">NeutralRole</rdfs:label>
		<rdfs:comment xml:lang="en">A neutral role is a role with impartial intent.</rdfs:comment>
		<sh:targetClass rdf:resource="https://ontology.unifiedcyberontology.org/uco/role/NeutralRole"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="https://ontology.unifiedcyberontology.org/uco/role/Role">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
		<rdf:type rdf:resource="http://www.w3.org/ns/shacl#NodeShape"/>
		<rdfs:subClassOf rdf:resource="https://ontology.unifiedcyberontology.org/uco/core/UcoObject"/>
		<rdfs:label xml:lang="en">Role</rdfs:label>
		<rdfs:comment xml:lang="en">A role is a usual or customary function based on contextual perspective.</rdfs:comment>
		<sh:targetClass rdf:resource="https://ontology.unifiedcyberontology.org/uco/role/Role"/>
	</rdf:Description>

</rdf:RDF>