		<rdfs:subClassOf rdf:resource="https://ontology.unifiedcyberontology.org/uco/core/UcoObject"/>
		<rdfs:label xml:lang="en">ControlledVocabulary</rdfs:label>
		<rdfs:comment xml:lang="en">A controlled vocabulary is an explicitly constrained set of string values.</rdfs:comment>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
				<sh:maxCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="http://www.w3.org/ns/shacl#Literal"/>
				<sh:path rdf:resource="https://ontology.unifiedcyberontology.org/uco/core/constrainingVocabularyReference"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
				<sh:maxCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</sh:maxCount>
				<sh:minCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</sh:minCount>
				<sh:nodeKind rdf:resource="http://www.w3.org/ns/shacl#Literal"/>
				<sh:path rdf:resource="https://ontology.unifiedcyberontology.org/uco/core/value"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
				<sh:maxCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="http://www.w3.org/ns/shacl#Literal"/>
				<sh:path rdf:resource="https://ontology.unifiedcyberontology.org/uco/core/constrainingVocabularyName"/>
			</rdf:Description>
		</sh:property>
		<sh:targetClass rdf:resource="https://ontology.unifiedcyberontology.org/uco/core/ControlledVocabulary"/>
	</rdf:Description>
	
