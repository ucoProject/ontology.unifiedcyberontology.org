<?xml version="1.0" encoding="UTF-8"?>

<rdf:RDF
	xmlns:core="https://ontology.unifiedcyberontology.org/uco/core/"
	xmlns:identity="https://ontology.unifiedcyberontology.org/uco/identity/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:sh="http://www.w3.org/ns/shacl#"
	xmlns:tool="https://ontology.unifiedcyberontology.org/uco/tool/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://ontology.unifiedcyberontology.org/uco/tool">
		<rdfs:label xml:lang="en">uco-tool</rdfs:label>
		<owl:imports rdf:resource="https://ontology.unifiedcyberontology.org/uco/core"/>
	</owl:Ontology>
	
	<rdf:Description rdf:about="&tool;AnalyticTool">
		<rdf:type rdf:resource="&owl;Class"/>
		<rdf:type rdf:resource="&sh;NodeShape"/>
		<rdfs:subClassOf rdf:resource="&tool;Tool"/>
		<rdfs:label xml:lang="en">AnalyticTool</rdfs:label>
		<rdfs:comment xml:lang="en">An analytic tool is an artifact of hardware and/or software utilized to accomplish a task or purpose of explanation, interpretation or logical reasoning.</rdfs:comment>
		<sh:targetClass rdf:resource="&tool;AnalyticTool"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&tool;BuildConfigurationType">
		<rdf:type rdf:resource="&owl;Class"/>
		<rdf:type rdf:resource="&sh;NodeShape"/>
		<rdfs:label xml:lang="en">BuildConfigurationType</rdfs:label>
		<rdfs:comment xml:lang="en">A build configuration type is a characterization of how a particular version of software can or should be built.</rdfs:comment>
		<sh:property>
			<rdf:Description>
				<sh:class rdf:resource="&tool;ConfigurationSettingType"/>
				<sh:nodeKind rdf:resource="&sh;BlankNodeOrIRI"/>
				<sh:path rdf:resource="&tool;configurationSettings"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;configurationSettingDescription"/>
			</rdf:Description>
		</sh:property>
		<sh:targetClass rdf:resource="&tool;BuildConfigurationType"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&tool;BuildFacet">
		<rdf:type rdf:resource="&owl;Class"/>
		<rdf:type rdf:resource="&sh;NodeShape"/>
		<rdfs:subClassOf rdf:resource="&core;Facet"/>
		<rdfs:label xml:lang="en">BuildFacet</rdfs:label>
		<rdfs:comment xml:lang="en">A build facet is a grouping of characteristics unique to a particular version of a software.</rdfs:comment>
		<sh:property>
			<rdf:Description>
				<sh:class rdf:resource="&tool;BuildInformationType"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;BlankNodeOrIRI"/>
				<sh:path rdf:resource="&tool;buildInformation"/>
			</rdf:Description>
		</sh:property>
		<sh:targetClass rdf:resource="&tool;BuildFacet"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&tool;BuildInformationType">
		<rdf:type rdf:resource="&owl;Class"/>
		<rdf:type rdf:resource="&sh;NodeShape"/>
		<rdfs:label xml:lang="en">BuildInformationType</rdfs:label>
		<rdfs:comment xml:lang="en">A build information type is a grouping of characteristics that describe how a particular version of software was converted from source code to executable code.</rdfs:comment>
		<sh:property>
			<rdf:Description>
				<sh:class rdf:resource="&tool;BuildConfigurationType"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;BlankNodeOrIRI"/>
				<sh:path rdf:resource="&tool;buildConfiguration"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:class rdf:resource="&tool;BuildUtilityType"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;BlankNodeOrIRI"/>
				<sh:path rdf:resource="&tool;buildUtility"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:class rdf:resource="&tool;CompilerType"/>
				<sh:nodeKind rdf:resource="&sh;BlankNodeOrIRI"/>
				<sh:path rdf:resource="&tool;compilers"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:class rdf:resource="&tool;LibraryType"/>
				<sh:nodeKind rdf:resource="&sh;BlankNodeOrIRI"/>
				<sh:path rdf:resource="&tool;libraries"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;dateTime"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;compilationDate"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;buildID"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;buildLabel"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;buildOutputLog"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;buildProject"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;buildScript"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;buildVersion"/>
			</rdf:Description>
		</sh:property>
		<sh:targetClass rdf:resource="&tool;BuildInformationType"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&tool;BuildUtilityType">
		<rdf:type rdf:resource="&owl;Class"/>
		<rdf:type rdf:resource="&sh;NodeShape"/>
		<rdfs:label xml:lang="en">BuildUtilityType</rdfs:label>
		<rdfs:comment xml:lang="en">A build utility type characterizes the tool used to convert from source code to executable code for a particular version of software.</rdfs:comment>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:minCount rdf:datatype="&xsd;integer">1</sh:minCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;buildUtilityName"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;cpeid"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;swid"/>
			</rdf:Description>
		</sh:property>
		<sh:targetClass rdf:resource="&tool;BuildUtilityType"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&tool;CompilerType">
		<rdf:type rdf:resource="&owl;Class"/>
		<rdf:type rdf:resource="&sh;NodeShape"/>
		<rdfs:label xml:lang="en">CompilerType</rdfs:label>
		<rdfs:comment xml:lang="en">A compiler type is a grouping of characteristics unique to a specific program that translates computer code written in one programming language (the source language) into another language (the target language). Typically a program that translates source code from a high-level programming language to a lower-level language (e.g., assembly language, object code, or machine code) to create an executable program. [based on https://en.wikipedia.org/wiki/Compiler]</rdfs:comment>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;compilerInformalDescription"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;cpeid"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;swid"/>
			</rdf:Description>
		</sh:property>
		<sh:targetClass rdf:resource="&tool;CompilerType"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&tool;ConfigurationSettingType">
		<rdf:type rdf:resource="&owl;Class"/>
		<rdf:type rdf:resource="&sh;NodeShape"/>
		<rdfs:label xml:lang="en">ConfigurationSettingType</rdfs:label>
		<rdfs:comment xml:lang="en">A configuration setting type is a grouping of characteristics unique to a particular parameter or initial setting for the use of a tool, application, or other cyber object.</rdfs:comment>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:minCount rdf:datatype="&xsd;integer">1</sh:minCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;itemName"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:minCount rdf:datatype="&xsd;integer">1</sh:minCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;itemValue"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;itemDescription"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;itemType"/>
			</rdf:Description>
		</sh:property>
		<sh:targetClass rdf:resource="&tool;ConfigurationSettingType"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&tool;DefensiveTool">
		<rdf:type rdf:resource="&owl;Class"/>
		<rdf:type rdf:resource="&sh;NodeShape"/>
		<rdfs:subClassOf rdf:resource="&tool;Tool"/>
		<rdfs:label xml:lang="en">DefensiveTool</rdfs:label>
		<rdfs:comment xml:lang="en">A defensive tool is an artifact of hardware and/or software utilized to accomplish a task or purpose of guarding.</rdfs:comment>
		<sh:targetClass rdf:resource="&tool;DefensiveTool"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&tool;DependencyType">
		<rdf:type rdf:resource="&owl;Class"/>
		<rdf:type rdf:resource="&sh;NodeShape"/>
		<rdfs:label xml:lang="en">DependencyType</rdfs:label>
		<rdfs:comment xml:lang="en">A dependency type is a grouping of characteristics unique to something that a tool or other software relies on to function as intended.</rdfs:comment>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:minCount rdf:datatype="&xsd;integer">1</sh:minCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;dependencyDescription"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;dependencyType"/>
			</rdf:Description>
		</sh:property>
		<sh:targetClass rdf:resource="&tool;DependencyType"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&tool;LibraryType">
		<rdf:type rdf:resource="&owl;Class"/>
		<rdf:type rdf:resource="&sh;NodeShape"/>
		<rdfs:label xml:lang="en">LibraryType</rdfs:label>
		<rdfs:comment xml:lang="en">A library type is a grouping of characteristics unique to a collection of resources incorporated into the build of a software.</rdfs:comment>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:minCount rdf:datatype="&xsd;integer">1</sh:minCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;libraryName"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:minCount rdf:datatype="&xsd;integer">1</sh:minCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;libraryVersion"/>
			</rdf:Description>
		</sh:property>
		<sh:targetClass rdf:resource="&tool;LibraryType"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&tool;MaliciousTool">
		<rdf:type rdf:resource="&owl;Class"/>
		<rdf:type rdf:resource="&sh;NodeShape"/>
		<rdfs:subClassOf rdf:resource="&tool;Tool"/>
		<rdfs:label xml:lang="en">MaliciousTool</rdfs:label>
		<rdfs:comment xml:lang="en">A malicious tool is an artifact of hardware and/or software utilized to accomplish a malevolent task or purpose.</rdfs:comment>
		<sh:targetClass rdf:resource="&tool;MaliciousTool"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&tool;Tool">
		<rdf:type rdf:resource="&owl;Class"/>
		<rdf:type rdf:resource="&sh;NodeShape"/>
		<rdfs:subClassOf rdf:resource="&core;UcoObject"/>
		<rdfs:label xml:lang="en">Tool</rdfs:label>
		<rdfs:comment xml:lang="en">A tool is an element of hardware and/or software utilized to carry out a particular function.</rdfs:comment>
		<sh:property>
			<rdf:Description>
				<sh:class rdf:resource="&identity;Identity"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;BlankNodeOrIRI"/>
				<sh:path rdf:resource="&tool;creator"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;anyURI"/>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;references"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;servicePack"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;toolType"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:maxCount rdf:datatype="&xsd;integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;version"/>
			</rdf:Description>
		</sh:property>
		<sh:targetClass rdf:resource="&tool;Tool"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&tool;ToolConfigurationTypeFacet">
		<rdf:type rdf:resource="&owl;Class"/>
		<rdf:type rdf:resource="&sh;NodeShape"/>
		<rdfs:subClassOf rdf:resource="&core;Facet"/>
		<rdfs:label xml:lang="en">ToolConfigurationTypeFacet</rdfs:label>
		<rdfs:comment xml:lang="en">A tool configuration type facet is a grouping of characteristics unique to the instantial settings and setup of a tool.</rdfs:comment>
		<sh:property>
			<rdf:Description>
				<sh:class rdf:resource="&tool;DependencyType"/>
				<sh:nodeKind rdf:resource="&sh;BlankNodeOrIRI"/>
				<sh:path rdf:resource="&tool;dependencies"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="&xsd;string"/>
				<sh:nodeKind rdf:resource="&sh;Literal"/>
				<sh:path rdf:resource="&tool;usageContextAssumptions"/>
			</rdf:Description>
		</sh:property>
		<sh:targetClass rdf:resource="&tool;ToolConfigurationTypeFacet"/>
	</rdf:Description>
	
	<owl:ObjectProperty rdf:about="&tool;buildConfiguration">
		<rdfs:label xml:lang="en">buildConfiguration</rdfs:label>
		<rdfs:comment xml:lang="en">How the build utility was configured for a particular build of a particular software.</rdfs:comment>
		<rdfs:range rdf:resource="&tool;BuildConfigurationType"/>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty rdf:about="&tool;buildID">
		<rdfs:label xml:lang="en">buildID</rdfs:label>
		<rdfs:comment xml:lang="en">An externally defined unique identifier for a particular build of a software.</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;string"/>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty rdf:about="&tool;buildInformation">
		<rdfs:label xml:lang="en">buildInformation</rdfs:label>
		<rdfs:comment xml:lang="en">Describes how a particular tool was built.</rdfs:comment>
		<rdfs:range rdf:resource="&tool;BuildInformationType"/>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty rdf:about="&tool;buildLabel">
		<rdfs:label xml:lang="en">buildLabel</rdfs:label>
		<rdfs:comment xml:lang="en">Relevant label for a particular build of a particular software.</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;string"/>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&tool;buildOutputLog">
		<rdfs:label xml:lang="en">buildOutputLog</rdfs:label>
		<rdfs:comment xml:lang="en">The output log of the build process for a software.</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;string"/>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&tool;buildProject">
		<rdfs:label xml:lang="en">buildProject</rdfs:label>
		<rdfs:comment xml:lang="en">The project name of a build of a software.</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;string"/>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&tool;buildScript">
		<rdfs:label xml:lang="en">buildScript</rdfs:label>
		<rdfs:comment xml:lang="en">The actual build script for a particular build of a particular software.</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;string"/>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty rdf:about="&tool;buildUtility">
		<rdfs:label xml:lang="en">buildUtility</rdfs:label>
		<rdfs:comment xml:lang="en">Identifies the utility used to build a software.</rdfs:comment>
		<rdfs:range rdf:resource="&tool;BuildUtilityType"/>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty rdf:about="&tool;buildUtilityName">
		<rdfs:label xml:lang="en">buildUtilityName</rdfs:label>
		<rdfs:comment xml:lang="en">The informally defined name of the utility used to build a particular software.</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;string"/>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&tool;buildVersion">
		<rdfs:label xml:lang="en">buildVersion</rdfs:label>
		<rdfs:comment xml:lang="en">The appropriate version descriptor of a particular build of a particular software.</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;string"/>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&tool;compilationDate">
		<rdfs:label xml:lang="en">compilationDate</rdfs:label>
		<rdfs:comment xml:lang="en">The compilation date for the build of a software.</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;dateTime"/>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&tool;compilerInformalDescription">
		<rdfs:label xml:lang="en">compilerInformalDescription</rdfs:label>
		<rdfs:comment xml:lang="en">An informal description of a compiler.</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;string"/>
	</owl:DatatypeProperty>
	
