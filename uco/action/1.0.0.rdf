				<sh:nodeKind rdf:resource="http://www.w3.org/ns/shacl#Literal"/>
				<sh:path rdf:resource="https://ontology.unifiedcyberontology.org/uco/action/units"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="https://ontology.unifiedcyberontology.org/uco/vocabulary/TrendVocab"/>
				<sh:message>Value is outside the default vocabulary TrendVocab.</sh:message>
				<sh:path rdf:resource="https://ontology.unifiedcyberontology.org/uco/action/trend"/>
