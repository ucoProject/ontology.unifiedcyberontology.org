				</sh:or>
				<sh:path rdf:resource="https://ontology.unifiedcyberontology.org/uco/action/trend"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:message>Value is not member of the vocabulary TrendVocab.</sh:message>
				<sh:or rdf:parseType="Collection">
					<rdf:Description>
						<sh:datatype rdf:resource="https://ontology.unifiedcyberontology.org/uco/vocabulary/TrendVocab"/>
