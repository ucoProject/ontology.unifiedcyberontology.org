				<sh:class rdf:resource="https://ontology.unifiedcyberontology.org/uco/core/UcoObject"/>
				<sh:nodeKind rdf:resource="http://www.w3.org/ns/shacl#IRI"/>
				<sh:path rdf:resource="https://ontology.unifiedcyberontology.org/uco/action/object"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:class rdf:resource="https://ontology.unifiedcyberontology.org/uco/core/UcoObject"/>
				<sh:nodeKind rdf:resource="http://www.w3.org/ns/shacl#IRI"/>
				<sh:path rdf:resource="https://ontology.unifiedcyberontology.org/uco/action/participant"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:class rdf:resource="https://ontology.unifiedcyberontology.org/uco/core/UcoObject"/>
				<sh:nodeKind rdf:resource="http://www.w3.org/ns/shacl#IRI"/>
				<sh:path rdf:resource="https://ontology.unifiedcyberontology.org/uco/action/result"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:class rdf:resource="https://ontology.unifiedcyberontology.org/uco/location/Location"/>
				<sh:nodeKind rdf:resource="http://www.w3.org/ns/shacl#IRI"/>
				<sh:path rdf:resource="https://ontology.unifiedcyberontology.org/uco/action/location"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
				<sh:maxCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="http://www.w3.org/ns/shacl#Literal"/>
				<sh:path rdf:resource="https://ontology.unifiedcyberontology.org/uco/action/endTime"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
				<sh:maxCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="http://www.w3.org/ns/shacl#Literal"/>
				<sh:path rdf:resource="https://ontology.unifiedcyberontology.org/uco/action/startTime"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
				<sh:maxCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="http://www.w3.org/ns/shacl#Literal"/>
				<sh:path rdf:resource="https://ontology.unifiedcyberontology.org/uco/action/actionCount"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:datatype rdf:resource="https://ontology.unifiedcyberontology.org/uco/vocabulary/ActionStatusTypeVocab"/>
				<sh:message>Value is outside the default vocabulary ActionStatusTypeVocab.</sh:message>
				<sh:path rdf:resource="https://ontology.unifiedcyberontology.org/uco/action/actionStatus"/>
				<sh:severity rdf:resource="http://www.w3.org/ns/shacl#Info"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:maxCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</sh:maxCount>
				<sh:nodeKind rdf:resource="http://www.w3.org/ns/shacl#Literal"/>
				<sh:or rdf:parseType="Collection">
					<rdf:Description>
						<sh:datatype rdf:resource="https://ontology.unifiedcyberontology.org/uco/vocabulary/ActionStatusTypeVocab"/>
					</rdf:Description>
					<rdf:Description>
						<sh:datatype rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
					</rdf:Description>
				</sh:or>
				<sh:path rdf:resource="https://ontology.unifiedcyberontology.org/uco/action/actionStatus"/>
			</rdf:Description>
		</sh:property>
		<sh:property>
			<rdf:Description>
				<sh:message>Value is not member of the vocabulary ActionStatusTypeVocab.</sh:message>
				<sh:or rdf:parseType="Collection">
					<rdf:Description>
						<sh:datatype rdf:resource="https://ontology.unifiedcyberontology.org/uco/vocabulary/ActionStatusTypeVocab"/>
						<sh:in>
							<rdf:Description>
								<rdf:first rdf:datatype="https://ontology.unifiedcyberontology.org/uco/vocabulary/ActionStatusTypeVocab">Complete/Finish</rdf:first>
								<rdf:rest>
									<rdf:Description>
										<rdf:first rdf:datatype="https://ontology.unifiedcyberontology.org/uco/vocabulary/ActionStatusTypeVocab">Error</rdf:first>
										<rdf:rest>
											<rdf:Description>
												<rdf:first rdf:datatype="https://ontology.unifiedcyberontology.org/uco/vocabulary/ActionStatusTypeVocab">Fail</rdf:first>
												<rdf:rest>
													<rdf:Description>
														<rdf:first rdf:datatype="https://ontology.unifiedcyberontology.org/uco/vocabulary/ActionStatusTypeVocab">Ongoing</rdf:first>
														<rdf:rest>
															<rdf:Description>
																<rdf:first rdf:datatype="https://ontology.unifiedcyberontology.org/uco/vocabulary/ActionStatusTypeVocab">Pending</rdf:first>
																<rdf:rest>
																	<rdf:Description>
																		<rdf:first rdf:datatype="https://ontology.unifiedcyberontology.org/uco/vocabulary/ActionStatusTypeVocab">Success</rdf:first>
																		<rdf:rest>
																			<rdf:Description>
																				<rdf:first rdf:datatype="https://ontology.unifiedcyberontology.org/uco/vocabulary/ActionStatusTypeVocab">Unknown</rdf:first>
																				<rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
																			</rdf:Description>
																		</rdf:rest>
																	</rdf:Description>
																</rdf:rest>
															</rdf:Description>
														</rdf:rest>
													</rdf:Description>
												</rdf:rest>
											</rdf:Description>
										</rdf:rest>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</sh:in>
					</rdf:Description>
					<rdf:Description>
						<sh:datatype rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
					</rdf:Description>
				</sh:or>
				<sh:path rdf:resource="https://ontology.unifiedcyberontology.org/uco/action/actionStatus"/>
			</rdf:Description>
		</sh:property>
		<sh:targetClass rdf:resource="https://ontology.unifiedcyberontology.org/uco/action/Action"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="https://ontology.unifiedcyberontology.org/uco/action/ActionArgumentFacet">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
